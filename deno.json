{
  "name": "@chesapeake/dn",
  "version": "0.0.9",
  "description": "dn cli",
  "exports": {
    ".": "./sdk/mod.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "sdk/**/*.ts",
      "docs/**/*.md",
      "docs/img/**"
    ],
    "exclude": [
      "docs/deslop.md",
      "docs/hung-process-triage.md"
    ]
  },
  "tasks": {
    "dev": "deno run --watch cli/main.ts",
    "lint": "deno lint **/*.ts",
    "typecheck": "deno check **/*.ts",
    "fmt": "deno fmt **/*.ts",
    "precommit": "deno fmt && deno lint && deno check **/*.ts",
    "publish": "deno publish"
  },
  "imports": {
    "@chesapeake/obsidian-gql": "jsr:@chesapeake/obsidian-gql@^0.2.0",
    "$dax": "jsr:@david/dax@^0.45.0",
    "@libsql/client": "npm:@libsql/client@0.7.0",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@zaubrik/djwt": "jsr:@zaubrik/djwt@^3.0.2"
  }
}
